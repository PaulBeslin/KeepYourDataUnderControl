{
  "name": "Keep Your Data Under Control",
  "description" : "Description",
  "version": "0.2",

    "permissions": [
        "contextMenus",
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
  "content_scripts": [
    {
      "all_frames" : true,
      "run_at": "document_start",
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
        "js": [
            "jquery-3.5.1.min.js",
            "qr.min.js",
            "qrdecode.js",
            "QrCode.js",
            "content_script-1.js",
            "text_encoding.js"
        ]
    }
  ],

  "web_accessible_resources": [ 
    "jquery-3.5.1.min.js"
  ],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
